{
	"name": "moduloze",
	"description": "Convert CommonJS (CJS) modules to UMD and ESM formats",
	"version": "0.9.1",
	"main": "src/index.js",
	"bin": {
		"mz": "./scripts/mz"
	},
	"scripts": {
		"prepare": "npm run build",
		"build-all": "node scripts/build-all.js",
		"build": "npm run build-all"
	},
	"dependencies": {
		"@babel/generator": "~7.13.9",
		"@babel/parser": "~7.13.11",
		"@babel/template": "~7.12.13",
		"@babel/traverse": "~7.13.0",
		"@babel/types": "~7.13.0",
		"dotenv": "~8.2.0",
		"micromatch": "~4.0.2",
		"minimist": "~1.2.5",
		"mkdirp": "~1.0.4",
		"recursive-readdir-sync": "~1.0.6",
		"terser": "~5.15.0",
		"toposort": "~2.0.2"
	},
	"mz-dependencies": {
		"cli.js": "CLI",
		"index.js": "Mz",
		"umd.js": "UMD",
		"esm.js": "ESM",
		"analysis.js": "Analysis",
		"helpers.js": "Helpers",
		":::path": "NodePath",
		":::fs": "NodeFS",
		":::os": "NodeOS",
		":::@babel/generator": "BabelGenerator",
		":::@babel/parser": "BableParser",
		":::@babel/template": "BabelTemplate",
		":::@babel/traverse": "BabelTraverse",
		":::@babel/types": "BabelTypes",
		":::dotenv": "DotEnv",
		":::micromatch": "MicroMatch",
		":::minimist": "Minimist",
		":::mkdirp": "MkDirP",
		":::recursive-readdir-sync": "RecursiveReaddirSync",
		":::terser": "Terser",
		":::toposort": "TopoSort"
	},
	"repository": "getify/moduloze",
	"keywords": [
		"cjs",
		"esm",
		"umd",
		"module",
		"transpiler",
		"convert"
	],
	"bugs": {
